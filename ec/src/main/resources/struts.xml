<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.multipart.maxSize" value="10485760"/>
	<package name="webpage" namespace="/" extends="struts-default">
		<action name="" class="IndexAction">
			<result name="success">/webPage/index.jsp</result>
		</action>
		<action name="MyOrder" class="MyOrderAction">
			<result name="ViewOrderSuccess">/webPage/ViewOrder/jsp</result>
			</action>
	</package>
</struts>

